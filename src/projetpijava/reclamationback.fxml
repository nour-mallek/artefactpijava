<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projetpijava.ReclamationbackController">
    <stylesheets>
        <URL value="@reclamationback.css" />
    </stylesheets>
   <children>
      <ImageView fitHeight="616.0" fitWidth="885.0" layoutX="-60.0" layoutY="-62.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Icons/back.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="tvrecla" layoutX="204.0" layoutY="40.0" onMouseClicked="#select" prefHeight="341.0" prefWidth="429.0">
        <columns>
          <TableColumn fx:id="userid" prefWidth="75.0" text="user" />
          <TableColumn fx:id="textrecla" prefWidth="353.0" text="text" />
        </columns>
      </TableView>
      <Button fx:id="affiche" layoutX="274.0" layoutY="409.0" mnemonicParsing="false" onAction="#affiche" text="affiche" />
      <Button fx:id="delete" layoutX="495.0" layoutY="409.0" mnemonicParsing="false" onAction="#delete" text="Supprimer" />
   </children>
</AnchorPane>
